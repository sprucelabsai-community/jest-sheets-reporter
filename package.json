{
  "name": "@sprucelabs/jest-sheets-reporter",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build"
  ],
  "version": "1.2.9",
  "main": "./build/SheetsReporter.js",
  "types": "./build/SheetsReporter.d.ts",
  "description": "Have your Jest tests update a Google Spreadsheetâ„¢Â®Â©. ðŸ“¡",
  "keywords": [
    "node",
    "components",
    "sprucebot",
    "sprucelabs"
  ],
  "homepage": "https://github.com/sprucelabsai/jest-sheets-reporter",
  "bugs": {
    "url": "https://github.com/sprucelabsai/jest-sheets-reporter/issues"
  },
  "scripts": {
    "arkit": "arkit -f index.ts -o docs/images/arkit.svg",
    "arkit.png": "arkit -f index.ts -o docs/images/arkit.png",
    "build": "yarn run clean && yarn run build.ts",
    "build.ts": "tsc",
    "clean": "rm -rf build/",
    "fix.lint": "eslint --fix '**/*.ts'",
    "lint": "eslint '**/*.ts' && tsc -p . --noEmit",
    "release": "semantic-release",
    "test": "jest",
    "watch.build": "chokidar src/* -c 'yarn run build'",
    "watch.test": "jest --watch",
    "upgrade.packages": "yarn-upgrade-all && rm -f yarn.lock ; yarn ; yarn fix.lint | true",
    "upgrade.packages.all": "yarn install && yarn upgrade.packages",
    "upgrade.packages.test": "yarn upgrade.packages.all && yarn lint && yarn build && yarn test"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "google-spreadsheet": "^3.1.15",
    "yarn-upgrade-all": "^0.5.4"
  },
  "devDependencies": {
    "@sprucelabs/semantic-release": "latest",
    "@sprucelabs/test": "latest",
    "@types/google-spreadsheet": "^3.0.2",
    "arkit": "^1.6.4",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.19.0",
    "eslint-config-spruce": "latest",
    "jest": "^26.6.3",
    "jest-reporters": "^0.0.2",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "jest": {
    "testTimeout": 300000,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/tmp/",
      "<rootDir>/src/",
      "<rootDir>/node_modules/",
      "<rootDir>/build/__tests__/testDirsAndFiles/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "moduleNameMapper": {
      "^#spruce/schemas/fields/(.*)": "<rootDir>/node_modules/@sprucelabs/schema/build/.spruce/schemas/fields/$1",
      "^#spruce/(.*)$": "<rootDir>/build/.spruce/$1"
    }
  },
  "yarn-upgrade-all": {
    "ignore": [
      "@sprucelabs/semantic-release",
      "@sprucelabs/test",
      "eslint-config-spruce"
    ]
  }
}
